<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<fx:root type="javafx.scene.layout.HBox" fx:id="projectRowHbox" spacing="10" xmlns:fx="http://javafx.com/fxml"
         alignment="CENTER_LEFT">

    <Label fx:id="projectName" maxWidth="165" minWidth="165" wrapText="true"/>
    <Button fx:id="editBtn" maxHeight="20" minWidth="20" style="-fx-background-color: none">
        <graphic>
            <ImageView fx:id="editIcon" fitHeight="20" fitWidth="16"/>
        </graphic>
    </Button>
    <Button fx:id="exportBtn" maxHeight="20" minWidth="20" style="-fx-background-color: none">
        <graphic>
            <ImageView fx:id="exportIcon" fitHeight="20" fitWidth="16"/>
        </graphic>
    </Button>
    <Button fx:id="deleteBtn" maxHeight="20" minWidth="20" style="-fx-background-color: none">
        <graphic>
            <ImageView fx:id="deleteIcon" fitHeight="20" fitWidth="16"/>
        </graphic>
    </Button>
</fx:root>
